#sum.hmmm
#lists all numbers from 1 to n and gives its sum
0	read r1		#Read n
1	setn r2 0	#Set i = 0
2	setn r3 0	#Set sum = 0
3	jeqzn r1 9	#if r1 == 0, break loop
4	addn r2 1	#i = i + 1
5	write r2	#print i
6	add r3 r3 r2	#sum = sum + i
7	addn r1 -1	#n = n - 1
8	jumpn 3		#Goto line 3
9	write r3
10	halt